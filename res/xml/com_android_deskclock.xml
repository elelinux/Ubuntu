<resource-redirections>
    <!-- Ubunru Clock Colors -->
    <item name="color/cling_text">@color/deskclock_cling_text</item>
    <item name="color/fg_bar">@color/deskclock_fg_bar</item>
    <item name="color/main_timer_clock">@color/deskclock_main_timer_clock</item>
    <item name="color/screen_saver_color">@color/deskclock_screen_saver_color</item>
