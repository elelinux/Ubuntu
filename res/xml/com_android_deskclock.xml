    <item name="mipmap/ic_launcher_alarmclock">@mipmap/com_android_deskclock_ic_launcher_alarmclock</item>
    <item name="mipmap/ic_launcher_timer">@mipmap/com_android_deskclock_ic_launcher_timer</item>
